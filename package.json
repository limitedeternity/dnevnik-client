{
  "name": "dnevnik-client",
  "version": "1.0.0",
  "description": "An unofficial client for Dnevnik.Ru",
  "author": "Marise Hayashi <@limitedeternity>",
  "license": "MIT",
  "scripts": {
    "devSWBuild": "workbox generateSW workbox-config.dev.js",
    "devServer": "npm run devSWBuild && cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "buildProd": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prodSWBuild": "workbox generateSW workbox-config.prod.js",
    "heroku-postbuild": "npm run buildProd && npm run prodSWBuild",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limitedeternity/dnevnik-client.git"
  },
  "bugs": {
    "url": "https://github.com/limitedeternity/dnevnik-client/issues"
  },
  "homepage": "https://limitedeternity.github.io/dnevnik-client",
  "dependencies": {
    "anchorme": "^1.1.2",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "helmet": "^3.12.0",
    "secure-ls": "^1.1.0",
    "shrink-ray": "^0.1.3",
    "throng": "^4.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-touch": "2.0.0-beta.4",
    "vuex": "^3.0.1"
  },
  "engines": {
    "node": "9.7.1",
    "npm": "5.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.11",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "workbox-cli": "3.0.0-beta.2"
  }
}
